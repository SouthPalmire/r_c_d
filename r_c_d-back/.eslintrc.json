{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": 1,
        "class-methods-use-this": 1,
        "no-return-await": 1,
        "lines-between-class-members": 1,
        "comma-dangle": 0
    }
}
